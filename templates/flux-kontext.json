{
  "$schema": "https://raw.githubusercontent.com/ayan4m1/comfyui-provisioner/main/template.schema.json",
  "description": "An install with only Flux Kontext",
  "environment": {
    "OPEN_BUTTON_PORT": "1111",
    "OPEN_BUTTON_TOKEN": "1",
    "JUPYTER_DIR": "/",
    "DATA_DIRECTORY": "/workspace/",
    "PORTAL_CONFIG": "localhost:1111:11111:/:Instance Portal|localhost:8188:18188:/:ComfyUI|localhost:8080:18080:/:Jupyter|localhost:8080:8080:/terminals/1:Jupyter Terminal|localhost:8384:18384:/:Syncthing",
    "COMFYUI_ARGS": "--disable-auto-launch --port 18188 --enable-cors-header"
  },
  "name": "ComfyUI - Flux Kontext",
  "provision": {
    "clip_models": [
      "https://huggingface.co/comfyanonymous/flux_text_encoders/resolve/main/clip_l.safetensors"
    ],
    "diffusion_models": [
      "https://huggingface.co/Comfy-Org/flux1-kontext-dev_ComfyUI/resolve/main/split_files/diffusion_models/flux1-dev-kontext_fp8_scaled.safetensors"
    ],
    "esrgan_models": [
      "https://github.com/Phhofm/models/releases/download/4xNomos2_hq_dat2/4xNomos2_hq_dat2.pth",
      "https://github.com/Kim2091/Kim2091-Models/releases/download/2x-AnimeSharpV4/2x-AnimeSharpV4_RCAN.safetensors",
      "https://github.com/Kim2091/Kim2091-Models/releases/download/2x-AnimeSharpV2_Set/2x-AnimeSharpV2_RPLKSR_Sharp.pth",
      "https://github.com/xinntao/Real-ESRGAN/releases/download/v0.2.2.4/RealESRGAN_x4plus_anime_6B.pth"
    ],
    "nodes": ["https://github.com/ltdrdata/ComfyUI-Manager"],
    "pip_packages": ["xformers"],
    "text_encoder_models": [
      "https://huggingface.co/comfyanonymous/flux_text_encoders/resolve/main/t5xxl_fp8_e4m3fn.safetensors"
    ],
    "vae_models": [
      "https://huggingface.co/ffxvs/vae-flux/resolve/main/ae.safetensors"
    ],
    "workflows": [
      "https://raw.githubusercontent.com/ayan4m1/comfyui-provisioner/main/workflows/flux-kontext.json"
    ]
  },
  "runType": "jupyter",
  "script": "comfyui.sh",
  "size": 4e10,
  "tag": "vastai/comfy:cuda-12.8-auto"
}
